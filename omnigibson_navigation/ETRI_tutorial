----------- RUN SIMULATOR --------------
-- Open new Terminal

conda activate behavior
cd ~/BEHAVIOR-1K
python3 scripts/bridge.py

python3 scripts/save_map.py

---------- RUN ROS2 WS ---------------
-- Open new Terminal

cd ~/ros2_behavior
source install/setup.bash

ros2 launch nav2_bringup navigation_launch.py\
use_sim_time:=false\
map:=/home/luan/ros2_behavior/src/behavior/maps/trav_map.yaml

-- Open new Terminal

cd ~/ros2_behavior
source install/setup.bash
ros2 run behavior tf_publisher

-- Open new Terminal

cd ~/ros2_behavior
source install/setup.bash
ros2 run behavior navigate_node

